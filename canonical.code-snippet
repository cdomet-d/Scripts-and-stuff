{
	"canonical" : {

	"prefix": "CANON", 
	"body": [
		"#ifndef ${TM_FILENAME_BASE/(.*)/${1:/upcase}/}_HPP",
		"# define ${TM_FILENAME_BASE/(.*)/${1:/upcase}/}_HPP\n",
		"class ${TM_FILENAME_BASE}",
		"{",
		"private:\n",
		"public:\n",
		"};\n",
		"#endif\n"
	],
	"description": "Creates a CPP canonical class"
	
	}
}
